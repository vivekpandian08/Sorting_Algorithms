      "source": [
        "def insertion(a):\n",
        "  n=len(a)\n",
        "  for i in range(1,n):\n",
        "    temp=a[i]\n",
        "    j=i-1\n",
        "    while j>=0 and a[j]>temp:\n",
        "      a[j+1]=a[j]\n",
        "      j-=1\n",
        "    a[j+1]=temp\n",
        "\n",
        "  return a"
      ],
      "execution_count": 16,
      "outputs": []
    },
